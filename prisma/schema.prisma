// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user{
  id Int @id @default(autoincrement())
  email String @unique
  firstname String
  lasname String
  pasword String 


}

//sirve para comunicarce con la base datos sin nececidad de escribir el sql



model Group {
  id Int @id @default(autoincrement())
  name String @unique
  nationalTeams NationalTeam[]
}

model NationalTeam {
  id Int @id @default(autoincrement())
  name String @unique
  group Group @relation(fields: [groupId], references: [id])
  groupId Int
}